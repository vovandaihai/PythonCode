import scrapy


class ProductScraperItem(scrapy.Item):
    # define the fields for your item here like:
    name = scrapy.Field()
    id = scrapy.Field()
    price = scrapy.Field()
    salePrice = scrapy.Field()
    images = scrapy.Field()
    size = scrapy.Field()
    color = scrapy.Field()
    link = scrapy.Field()
